<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="horizontal" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:id="@+id/row">
	<LinearLayout android:id="@+id/linearLayout1"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:orientation="vertical" android:paddingRight="4px">
		<TextView android:layout_width="wrap_content"
			android:textColor="@color/texto_oscuro" android:text="TextView"
			android:layout_height="wrap_content" android:id="@+id/fecha"
			android:textSize="16dp"
			android:padding="3dp"></TextView>
		<TextView android:layout_width="wrap_content"
			android:textColor="@color/texto_oscuro" android:text="TextView"
			android:layout_height="wrap_content" android:id="@+id/hora"
			android:textSize="16dp"
			android:padding="3dp"></TextView>
	</LinearLayout>
	<LinearLayout android:id="@+id/linearLayout1"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:orientation="vertical" android:paddingBottom="5dp">
		<TextView android:id="@+id/nombre" android:textColor="@color/texto_oscuro"
			android:layout_width="wrap_content" android:text="TextView"
			android:layout_height="wrap_content"
			android:textSize="20dp"></TextView>
		<TextView android:layout_width="wrap_content"
			android:textColor="@color/texto_oscuro" android:text="TextView"
			android:layout_height="wrap_content" android:id="@+id/desc1"
			android:textSize="14dp"
			></TextView>
	</LinearLayout>
</LinearLayout>
